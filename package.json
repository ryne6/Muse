{
  "name": "muse",
  "version": "0.1.0",
  "description": "AI-powered desktop coding assistant",
  "main": "./out/main/index.js",
  "author": "Muse Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start:api": "bun src/api/index.ts",
    "start:dev": "./start-dev.sh",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.node.json",
    "typecheck:watch": "tsc --noEmit --watch",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:backup": "sqlite3 muse.db \".dump\" > backups/muse-$(date +%Y%m%d-%H%M%S).sql",
    "clean": "rm -rf out dist node_modules/.vite",
    "clean:db": "rm -f muse.db muse.db-wal muse.db-shm",
    "package": "npm run build && electron-builder",
    "package:mac": "npm run build && electron-builder --mac",
    "package:win": "npm run build && electron-builder --win",
    "package:linux": "npm run build && electron-builder --linux",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:main": "vitest -c vitest.config.main.mts",
    "test:renderer": "vitest -c vitest.config.renderer.mts",
    "test:api": "vitest -c vitest.config.api.mts",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@hono/node-server": "^1.19.9",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "axios": "^1.13.2",
    "better-sqlite3": "^9.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.45.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "hono": "^4.0.0",
    "lucide-react": "^0.312.0",
    "nanoid": "^5.0.4",
    "openai": "^6.16.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
    "uuid": "^13.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.17",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.0",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^20.3.9",
    "jsdom": "^24.1.3",
    "msw": "^2.12.7",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
